{
  "name": "linter-gcc-portuguese",
  "main": "./lib/main",
  "version": "0.8.8",
  "description": "Portuguese Linter for C and C++ source files using gcc / g++",
  "keywords": [
    "lint",
    "linter",
    "portuguese",
    "c",
    "c++",
    "gcc",
    "g++",
    "error"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ArchangelZeero/linter-gcc-portuguese"
  },
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinter"
      }
    }
  },
  "dependencies": {
    "sb-exec": "^1.0.4",
    "shelljs": "^0.5.0",
    "fs-extra": "^0.26.1",
    "tempfile": "^1.1.0",
    "split-string": "^0.1.0",
    "named-js-regexp": "^1.3.1"
  },
  "devDependencies": {
    "eslint": "latest"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "globals": {
      "atom": true
    }
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
